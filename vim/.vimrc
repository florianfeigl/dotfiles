set nu rnu
set nocompatible
filetype plugin indent on
syntax on
set shiftwidth=2
set tabstop=2
set expandtab
set nowrap
set ignorecase
set smartcase
set showcmd
set showmode
set showmatch
set hlsearch
set wildmenu
set autoindent
nnoremap <C-d> <C-d>zz
nnoremap <C-u> <C-u>zz
nnoremap <C-n> :Explore<CR>
